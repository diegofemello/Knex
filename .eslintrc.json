{
  "env": {
    "browser": true,
      "es2021": true
  },
  "extends": [
    "airbnb-base"
  ],
    "parserOptions": {
    "ecmaVersion": 12,
      "sourceType": "module"
    },
  "rules": {
    "no-console": "off",
      "linebreak-style": 0,
      "no-param-reassign": 0,
      "camelcase":0,

      //permitir funções sem nome
      "func-names": "off",

      "import/order": "off"

      // permitir 1 variavel sem parenteses
      // "arrow-parens": "off"

  }

  
}
